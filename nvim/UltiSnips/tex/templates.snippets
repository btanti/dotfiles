snippet notes2 "Notes template 2" b
\documentclass[11pt,letterpaper]{article}
\usepackage[lmargin=1in,rmargin=1in,tmargin=1in,bmargin=1in]{geometry}

% -------------------
% Packages
% -------------------
\usepackage{
	amsmath,			% Math Environments
	amssymb,			% Extended Symbols
	amsthm,			    	% Theorem Environments
	cancel,			    	% Use Cancels
	enumerate,		    	% Enumerate Environments
	graphicx,			% Include Images
	lastpage,			% Reference Lastpage
	multicol,			% Use Multi-columns
	multirow,			% Use Multi-rows
	xcolor,			    	% Color support
	enumitem,			% Better lists
	optidef,			% Optimization problem formatting
	fancyvrb, 			% Better verbatim
	todonotes,                      % Note-taking and markup
	cleveref,			% Referencing package
	hyperref,			% Hyperlinking
	booktabs			% Better table design
}


% -------------------
% Font
% -------------------
\usepackage[T1]{fontenc}
\usepackage{charter}

%\usepackage[T1]{fontenc}
%\usepackage{mathpazo}

%\usepackage[bitstream-charter]{mathdesign}
